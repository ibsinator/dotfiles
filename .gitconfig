[user]
    name = 
    email = 

[alias]
    lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen[%ci] %Cgreen(%cr) %C(cyan)<%an>%Creset'
    log-me = !UN=$(git config user.name)&& git log --author="\"$UN\"" --pretty=format:'%h %cd %s' --date=short
    wdiff = diff --word-diff=plain
